<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cx.model.models.Paper" table="t_paper" lazy="false">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<property name="enable" column="enable" type="boolean" not-null="true" />
		<property name="name" column="name" type="string" not-null="true" unique="true" />
		<many-to-one name="subject" class="com.cx.model.models.Subject" foreign-key="FK_paper_subject" index="IDX_paper_subjectId">
			<column name="subjectId" not-null="false" />
		</many-to-one>
		<property name="uploadTime" column="uploadTime" type="calendar" not-null="true" />
		<property name="flag" column="flag" type="string" not-null="true" />
	</class>
</hibernate-mapping> 